{
  "logging": {
    "level": "INFO",
    "file": "logs/orchestration.log",
    "format": "%(asctime)s [%(levelname)-8s] %(name)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S"
  },
  "arguments":   {
    "data_dir": "data",
    "folders": ["TUNSPEKT Labordaten_all"],
    "filename_ref": "Faro_ipm_lab_pos01 - Box1",
    "filename_mov": "Faro_ipm_lab_pos02 - Box1",
    "filename_singlecloud": "Faro_ipm_lab_pos01 - Box1",
    "mov_as_corepoints": true,
    "use_subsampled_corepoints": 1,
    "sample_size": 10000,
    "scale_strategy": "radius",
    "only_stats": true,
    "stats_singleordistance": "distance",
    "output_format": "excel",
    "project": "MARS",
    "normal_override": null,
    "proj_override": null,
    "use_existing_params": false,
    "outlier_detection_method": "rmse",
    "outlier_multiplicator": 3.0,
    "log_level": "DEBUG"
  }
}