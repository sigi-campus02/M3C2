{

  "$schema": "./config.schema.json",

  "logging": {
    "level": "DEBUG",
    "file": "logs/orchestration.log",
    "format": "%(asctime)s [%(levelname)-8s] %(name)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S"
  },

  "arguments":   {
    "data_dir": "data",
    "folders": ["0342-0349"],
    "filename_reference": "mov",
    "filename_comparison": "ref",
    "filename_singlecloud": "",
    "use_subsampled_corepoints": 1,
    "sample_size": 10000,
    "scale_strategy": "radius",
    "only_stats": false,
    "stats_singleordistance": "distance",
    "output_format": "excel",
    "project": "TEST",
    "normal_override": null,
    "proj_override": null,
    "use_existing_params": true,
    "outlier_detection_method": "rmse",
    "outlier_multiplicator": 3.0
  },

  "arguments_plot_specific_files": {
    "overlay_files": [
      "data/1130-1133/python_ref_m3c2_distances.txt",
      "data/1130-1133/python_ref_ai_m3c2_distances.txt"
    ],
    "overlay_outdir": "outputs/Test_Plots",
    "plot_types": ["histogram", "boxplot", "scatter", "heatmap"]
  },

    "arguments_plot_files_in_folder": {
    "folder": "data/Multi-Illumination",
    "overlay_outdir": "outputs/Test_Plots",
    "options": ["plot_hist", "plot_gauss", "plot_weibull", "plot_box", "plot_qq", "plot_grouped_bar"]
  },

    "arguments_plot_several_folders": {
    "data_dir": "data",
    "folders": ["0342-0349", "1130-1133"],
    "filenames_distances": ["python_ref_m3c2_distances.txt", "python_ref_ai_m3c2_distances.txt"],
    "overlay_outdir": "outputs/Test_Plots",
    "options": ["plot_hist", "plot_gauss", "plot_weibull", "plot_box", "plot_qq", "plot_grouped_bar"]
  }

}