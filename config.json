{

  "$schema": "./config.schema.json",

  "logging": {
    "level": "INFO",
    "file": "logs/orchestration.log",
    "format": "%(asctime)s [%(levelname)-8s] %(name)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S"
  },
  "arguments":   {
    "data_dir": "data/TUNSPEKT Labordaten_all/Handheld",
    "folders": ["box01"],
    "filename_ref": "",
    "filename_mov": "",
    "filename_singlecloud": "Handheld_georef_mw - box01",
    "mov_as_corepoints": true,
    "use_subsampled_corepoints": 1,
    "sample_size": 10000,
    "scale_strategy": "radius",
    "only_stats": true,
    "stats_singleordistance": "single",
    "output_format": "excel",
    "project": "TUNSPEKT",
    "normal_override": null,
    "proj_override": null,
    "use_existing_params": false,
    "outlier_detection_method": "rmse",
    "outlier_multiplicator": 3.0,
    "log_level": "DEBUG"
  }
}